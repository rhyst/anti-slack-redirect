{
  "name": "Anti Slack Redirect",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Converts slack redirect urls to real urls.",
  "homepage_url": "https://www.github.com/rhyst",
  "background": {
    "scripts": [
      "src/redir.js"
    ]
  },
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
   "128": "icons/icon128.png" 
  },
  "permissions": [
    "tabs",
    "https://slack-redir.net/*"
  ]
}